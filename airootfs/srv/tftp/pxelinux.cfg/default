UI vesamenu.c32
TIMEOUT 300
DISPLAY ./boot.msg
MENU TITLE Welcome to Phd.ate's EZIO
LABEL ezio
  MENU LABEL Boot from EZIO Server for disk cloning
  MENU DEFAULT
  LINUX ./kernel/linux
  INITRD ./kernel/intel-ucode.img,./kernel/initrd.img
  sysappend 2
LABEL local
  MENU LABEL Boot from local drive
  localboot 0

